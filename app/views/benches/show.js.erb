<% benches_game = BenchesGame.where(game_id: params[:game], bench_id: @benchmark.id).last %>
<% if params[:type] == "full" %>
    <% if params[:game] != "total" %>
        $('#gameTitle').html("<%= benches_game.game.name %>")
        charts = ['bar', 'fps', 'frametime', 'gpu', 'cpu']
        $.each(charts, function (i, value){
            chart = Chartkick.charts[value]
            if (typeof chart !== 'undefined') {
                if (value == "fps")
                    chart.updateData(<%= raw benches_game.fps %>)
                if (value == "frametime")
                    chart.updateData(<%= raw benches_game.frametime %>)
                if (value == "gpu")
                    chart.updateData(<%= raw benches_game.gpu %>)
                if (value == "cpu")
                    chart.updateData(<%= raw benches_game.cpu %>)
                if (value == "bar")
                    chart.updateData(<%= raw benches_game.bar %>)
            }
        })
        $("#fps").show(); $("#frametime").show(); $("#gpu").show(); $("#cpu").show();
        $("#gameStats").html("<%= j render partial: 'stats', locals: {types: benches_game.types, benches_game: benches_game} %>")
    <% else %>
        //$('#gameContent').html("<%#= j render partial: 'benches/total_graphs', locals: {benchmark: @benchmark} %>")
        chart = Chartkick.charts["bar"]
        chart.updateData(<%= raw @benchmark.totalbar %>)
        $('#gameTitle').html("Total")
        $("#fps").hide(); $("#frametime").hide(); $("#gpu").hide(); $("#cpu").hide();
        <% json_data = JSON(@benchmark.totalbar) %>

        $("#gameStats").html("<%= j render partial: 'stats_total', locals: {benches_game: @benchmark} %>")
    <% end %>
<% end %>